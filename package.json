{
    "name": "win-classic-theme",
    "displayName": "WinClassic Theme",
    "description": "Windows Classic flavored theme built around MacClassic Theme's color palette",
    "version": "0.0.9",
    "author": {
        "name": "disk0",
        "url": "https://github.com/disco0"
    },
    "publisher": "disk0",
    "categories": [
        "Themes"
    ],
    "homepage": "https://github.com/disco0/win-classic-theme/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/disco0/win-classic-theme.git"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "icon": "icon/icon.png",
    "galleryBanner": {
        "color": "#E0DED9",
        "theme": "light"
    },
    "contributes": {
        "themes": [
            {
                "label": "WinClassic",
                "uiTheme": "vs",
                "path": "./themes/WinClassic-color-theme.json"
            }
        ]
    },
    "keywords": [
        "MacClassic",
        "WindowsClassic",
        "light",
        "theme"
    ],
    "devDependencies": {
        "vsce": "^1.66.0",
        "vscode": "^1.1.36"
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "nvm": "~/.nvm/nvm-exec which node | xargs -J % ln -sf % ./.nvm-node && chmod +x ./.nvm-node"
    }
}
